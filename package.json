{
  "name": "nificdc",
  "version": "1.0.0",
  "description": "Oracle to NiFi to Elasticsearch CDC Pipeline",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:contract": "jest tests/contract",
    "test:integration": "jest tests/integration --runInBand",
    "test:regression": "jest tests/regression --runInBand",
    "test:e2e": "./scripts/run-e2e-test.sh",
    "test:e2e:run": "jest tests/e2e --runInBand --testTimeout=120000",
    "test:coverage": "jest --coverage",
    "test:perf": "./scripts/run-perf-test.sh",
    "test:perf:baseline": "jest tests/performance/scenarios/baseline.test.js --runInBand --testTimeout=300000",
    "test:perf:latency": "jest tests/performance/scenarios/latency.test.js --runInBand --testTimeout=300000",
    "test:perf:query": "jest tests/performance/scenarios/query-performance.test.js --runInBand --testTimeout=120000",
    "test:perf:load": "jest tests/performance/scenarios/load.test.js --runInBand --testTimeout=900000",
    "test:perf:stress": "jest tests/performance/scenarios/stress.test.js --runInBand --testTimeout=3600000",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "lint:sql": "node scripts/validate-sql-registry.js",
    "generate": "node scripts/generate-from-spec.js",
    "generate:all": "node scripts/generate-from-spec.js --all",
    "generate:new": "node scripts/generate-from-spec.js --new"
  },
  "keywords": [
    "cdc",
    "nifi",
    "oracle",
    "elasticsearch",
    "data-pipeline"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "oracledb": "^6.3.0"
  }
}
